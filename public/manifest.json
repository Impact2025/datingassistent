  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },

  "protocol_handlers": [
    {
      "protocol": "web+datingassistent",
      "url": "/protocol?type=%s"
    }
  ],

  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".gif", ".webp"]
      }
    }
  ],

  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },

  "handle_links": "preferred",

  "edge_side_panel": {
    "preferred_width": 400
  },

  "related_applications": [],

  "serviceworker": {
    "src": "/sw.js",
    "scope": "/",
    "type": "classic",
    "update_via_cache": "none"
  }
